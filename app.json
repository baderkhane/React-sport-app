{
  "expo": {
    "name": "Sports.fr",
    "slug": "sportsfr",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "4.0.0",
    "orientation": "default",
    "icon": "./assets/images/logo_icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "fr.newsweb.sports",
      "buildNumber" :"1"      
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "ts",
        "tsx",
        "svg"
      ]
    },
    "description": "Football, Rugby, Tennis, Formule 1, Basket, NBA, Golf, Rallyes WRC, Moto GP, Cyclisme,… Où que vous soyez, suivez toute l’actu sport en temps réel avec l’application Sports.fr !",
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "pilot-systems",
            "project": "sportsfr",
            "authToken": "c680eeec7b134a3b9f014e3fc5d80057c5eaafb5c65441b2becfab9e529a8969",
            "url": "https://sentry.pilotsystems.net"
          }
        }
      ]
    },
    "android": {
      "package": "com.newsweb.sports",
      "permissions": []
    }
  }
  
}
